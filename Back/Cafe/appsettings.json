{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft.AspNetCore": "Warning",
				"Microsoft.EntityFrameworkCore": "Warning"
			}
		}
	},
	"AllowedHosts": "*",
	"ValuesForPresentationMode": {
		"OrderLifeTimeSeconds": 20,
		"BookedATableMinutes": 5
	},
	"Constants": {
		"CorsPolicies": {
			"Dev": "CorsDev"
		},
		"GetOpenAPIUrl": "http://localhost:3005",
		"UserId": "UserId",
		"UserName": "UserName",
		"AuthCookieName": "AuthCookie",
		"EFCoreMiniProfilerBasePath": "/profiler",
		"AntiforgeryTokenResponseHeaderName": "X-CSRF-Token-Response",
		"AntiforgeryTokenCookieName": "CSRF-Token-Cookie",
		"AntiforgeryTokenRequestHeaderName": "X-CSRF-Token-Request",
		"ApiVersions": [
			"/api/v1"
		],
		"LoggingParams": {
			"UserIdentifier": "UserIdentifier"
		},
		"EnvironmentVariableNames": {
			"OutputPath": "OutputPath"
		},
		"DefaultPage": "/index.html",
		"RootSpaStaticFilesPath": "wwwroot"
	}
	
}
